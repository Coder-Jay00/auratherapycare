{
  "version": 2,
  "functions": {
    "server.js": {
      "includeFiles": [
        "index.html",
        "register.html",
        "therapist-dashboard.html",
        "customer-dashboard.html",
        "css/**",
        "js/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
